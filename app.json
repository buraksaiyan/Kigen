{
  "expo": {
    "name": "Kigen",
    "slug": "kigen",
    "scheme": "kigen",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E1116"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kigen.app"
    },
    "android": {
      "package": "com.kigen.app",
      "permissions": []
    },
    "extra": {
      "eas": {
        "projectId": "REPLACE_WITH_EAS_PROJECT_ID"
      }
    }
  }
}